//ashish_07
#include<bits/stdc++.h>
using namespace std;
int main(){
	int N,a=0,d=0;
	cin>>N;
	string s ;
	cin>>s;
	int l = s.length();
	for(int i=0;i<N;i++){
		//cout<<l;
		if(s[i] == 'A'){
			a++;
		}else{
			d++;
		}
	}
	if(a==d){
		cout<<"Friendship";
	}else if(a>d){
		cout<<"Anton";
	}else{
		cout<<"Danik";
	}
		
	
}
